# Borrowing a helper function from C language,
# to print text to the console.
@foreign("stdio.h", "printf")
void println(String message);

# Function that returns a string.
String get_message() {
  return "Hello, World!";
}

# Each program msut have a main function.
# This function is executed automatically.
int main() {
  # Printing to the console.
  println(get_message());

  # 0 means the program exited successfully.
  return 0;
}